    <main class="offer-other">
      <div class="wrapper-thinner">
        <div class="offer__cards">
          <div class="offer__card offer__card--other">
            <h3 class="offer__card-heading">Student</h3>
            <p class="offer__price">$69</p>
            <ul class="offer__card-ul">
              <li class="offer__card-option">
                <span class="offer__card-checkmark">✓</span> opcja w pakiecie
              </li>
              <li class="offer__card-option">
                <span class="offer__card-checkmark">✓</span> opcja w pakiecie
              </li>
              <li class="offer__card-option">
                <span class="offer__card-checkmark">✓</span> opcja w pakiecie
              </li>
              <li class="offer__card-option offer__card-option--gray">
                <span class="offer__card-dash">—</span> opcja poza pakietem
              </li>
              <li class="offer__card-option offer__card-option--gray">
                <span class="offer__card-dash">—</span> opcja poza pakietem
              </li>
            </ul>
            <button class="offer__choose-btn">Wybierz</button>
          </div>

          <div class="offer__card offer__card--other">
            <h3 class="offer__card-heading">Standard+</h3>
            <p class="offer__price">$149</p>
            <ul class="offer__card-ul">
              <li class="offer__card-option">
                <span class="offer__card-checkmark">✓</span> opcja w pakiecie
              </li>
              <li class="offer__card-option">
                <span class="offer__card-checkmark">✓</span> opcja w pakiecie
              </li>
              <li class="offer__card-option">
                <span class="offer__card-checkmark">✓</span> opcja w pakiecie
              </li>
              <li class="offer__card-option">
                <span class="offer__card-checkmark">✓</span> opcja w pakiecie
              </li>
              <li class="offer__card-option offer__card-option--gray">
                <span class="offer__card-dash">—</span> opcja poza pakietem
              </li>
            </ul>
            <button class="offer__choose-btn">Wybierz</button>
          </div>

          <div class="offer__card offer__card--other">
            <h3 class="offer__card-heading">Turbo</h3>
            <p class="offer__price">$499</p>
            <ul class="offer__card-ul offer__card-ul--right">
              <li class="offer__card-option gray">
                <span class="offer__card-checkmark">✓</span> pakiet premium +
              </li>
              <li class="offer__card-option">
                <span class="offer__card-checkmark">✓</span> opcja dodatkowa
              </li>
              <li class="offer__card-option">
                <span class="offer__card-checkmark">✓</span> opcja dodatkowa
              </li>
              <li class="offer__card-option">
                <span class="offer__card-checkmark">✓</span> opcja dodatkowa
              </li>
              <li class="offer__card-option">
                <span class="offer__card-checkmark">✓</span> opcja dodatkowa
              </li>
            </ul>
            <button class="offer__choose-btn">Wybierz</button>
          </div>
        </div>
      </div>
    </main>

